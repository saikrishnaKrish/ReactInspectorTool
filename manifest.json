{
  "manifest_version": 3,
  "name": "React Inspector Pro",
  "version": "2.6.1",
  "description": "High-fidelity React component inspector with JSX export, computed styles, and global search.",
  "author": "Sai Krishna Kanteti",
  "action": {
    "default_title": "React Inspector Pro"
  },
  "permissions": [
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "inject.js"
      ],
      "run_at": "document_end",
      "world": "MAIN"
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      }
    }
  }
}